  <entry xmlns:flickr="urn:flickr:user" xmlns:dc="http://purl.org/dc/elements/1.1/">
    <title>Who's There?</title>
    <link rel="alternate" type="text/html" href="https://www.flickr.com/photos/piedmont_fossil/51618609921/"/>
